{
  "name": "platform-os-documentation",
  "version": "2.0.0",
  "description": "Documentation for Platform OS",
  "private": true,
  "repository": "mdyd-dev/nearme-documentation",
  "scripts": {
    "start": "npx webpack-cli -w",
    "clean": "git clean -Xfq $npm_package_config_build_dir/*.{css,js}",
    "build": "NODE_ENV=production npx webpack-cli",
    "predeploy": "npm run clean && npm run build",
    "deploy": "marketplace-kit deploy -f",
    "test": "codeceptjs run --steps"
  },
  "config": {
    "build_dir": "marketplace_builder/assets"
  },
  "author": "PlatformOS",
  "homepage": "https://documentation.platform-os.com",
  "browserslist": [
    "last 2 versions",
    "iOS > 7",
    "Safari > 6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.54",
    "@babel/plugin-transform-object-assign": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "4.9.2",
    "postcss": "^7.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-csso": "^3.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-focus": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-pseudoelements": "^5.0.0",
    "sass-loader": "^7.0.3",
    "script-loader": "^0.7.2",
    "style-loader": "^0.21.0",
    "webpack": "4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-require-from": "^1.3.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "prismjs": "^1.15.0",
    "webfontloader": "^1.6.28"
  },
  "peerDependencies": {
    "mocha": "^5.2.0",
    "unirest": "^0.5.1",
    "puppeteer": "^1.5.0",
    "codeceptjs": "^1.3.1"
  }
}
