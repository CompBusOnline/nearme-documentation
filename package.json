{
  "name": "platform-os-documentation",
  "version": "2.1.0",
  "description": "Documentation for PlatformOS",
  "private": true,
  "repository": "mdyd-dev/nearme-documentation",
  "scripts": {
    "start": "npx webpack-cli -w",
    "clean": "git clean -Xfq $npm_package_config_build_dir/*.{css,js}",
    "autogenerate": "time ./scripts/generate_all.sh",
    "build": "NODE_ENV=production npx webpack-cli",
    "deploy": "npm run clean && npm run build && marketplace-kit deploy -f",
    "test": "docker run -v $PWD/tests/e2e:/tests -it testcafe/testcafe \"chromium --no-sandbox,firefox:headless\" /tests/*.js",
    "test-ci": "docker run -v $PWD/tests/e2e:/tests testcafe/testcafe \"chromium --no-sandbox,firefox:headless\" /tests/*.js",
    "test-codecept": "codeceptjs run --steps"
  },
  "config": {
    "build_dir": "marketplace_builder/assets"
  },
  "author": "PlatformOS",
  "homepage": "https://documentation.platform-os.com",
  "browserslist": [
    "cover 90%",
    "last 2 chrome versions",
    "last 2 edge major versions",
    "last 2 ie major versions",
    "last 2 firefox major versions",
    "last 4 safari major versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "8.0.4",
    "css-loader": "^1.0.1",
    "css-mqpacker": "^7.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "4.9.4",
    "postcss": "^7.0.5",
    "postcss-calc": "^7.0.1",
    "postcss-csso": "^3.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-focus": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-require-from": "^1.4.0"
  },
  "dependencies": {
    "@sentry/browser": "^4.2.3",
    "anchor-js": "^4.1.1",
    "bootstrap": "^4.1.3",
    "ejs": "^2.6.1",
    "metismenujs": "^1.0.3",
    "prismjs": "^1.15.0",
    "turbolinks": "^5.2.0"
  },
  "peerDependencies": {
    "mocha": "^5.2.0",
    "unirest": "^0.5.1",
    "puppeteer": "^1.5.0",
    "codeceptjs": "^1.3.1"
  }
}
